#way1:
def eligible(age):
 if age>18:
    print ('Yes')
 else:
    print('No')
age=int(input('Enter age value:'))
eligible(age)
  
#way2:
def eligible(age):
 if age>18:
    return 'Yes'
 else:
    return'No'
age=int(input('Enter age value:'))
print (eligible(age))
  
